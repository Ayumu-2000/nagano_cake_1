<h1>商品新規登録</h1>
<%= form_with model: @item, url: admin_items_path, method: :post do |f| %>
  <label for="image">商品画像</label><br>
  <%= f.file_field :item_image, accept: "image/*" %><br>
  <label for="name">商品名</label><br>
  <%= f.text_field :name %><br>
  <label for="introduction">商品説明</label><br>
  <%= f.text_area :introduction %><br>
  <label for="price">税抜価格</label><br>
  <%= f.text_field :price %><br>
  <%= f.submit '新規登録' %><br>
<% end %>
